  "customEvaluationSchemeConfig": {
    "functionId": "BeatSinewave",
    "sampleIntervalMin": 0,
    "sampleIntervalMax": 50.265,
    "sampleResolution": 160,
    "gradientMseWeight": 0.9
  }
